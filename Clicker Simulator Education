
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local NetworkLoader = game:GetService("ReplicatedStorage").Modules.Utilities.ServiceLoader.NetworkService
local UtilLoader = game:GetService("ReplicatedStorage").Modules.Utilities.UtilityLoader
local Network = require(NetworkLoader)
local Util = require(UtilLoader)
local Click = Network.GetFunction("ClickFunction")
local Rebirth = Network.GetEvent("RebirthEvent")
local Dispenser = Util.GetUtility("dispenserUtility")
local vu = game:GetService("VirtualUser")
local LocalPlayer = game:GetService("Players").LocalPlayer

local walkSpeedEnabled = false
local jumpPowerEnabled = false
local oldSpeed = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed
local walkSpeed = oldSpeed
local oldJumpPower = game.Players.LocalPlayer.Character.Humanoid.JumpPower
local jumpPower = oldJumpPower
local autoClick = false
local autoRebirth = false
local amount = 1
local autoEgg = false 


local Window = OrionLib:MakeWindow({Name = "Clicking Simulator Education On Scripting ", HidePremium = true, SaveConfig = true, ConfigFolder = "ClickerSus"})

local Tab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local Section = Tab:AddSection({
    Name = "Features"
})

Tab:AddToggle({
    Name = "AutoClick",
    Default = false,
    Callback = function(Value)
        autoClick = Value
        if Value then
            repeat
                Click:Invoke("press")
                task.wait(0.01)
            until not autoClick
        end 
    end
})

Tab:AddToggle({
    Name = "AutoRebirth",
    Default = false,
    Callback = function(Value)
        autoRebirth = Value
        if Value then
            repeat
                Rebirth:Fire(amount)
                task.wait(0.1)
            until not autoRebirth
        end 
    end
})

Tab:AddDropdown({
	Name = "Rebirths",
	Default = "1",
	Options = {"1", "5","20","80","400","2000","10000","40000","100000","400000"},
	Callback = function(Value)
		amount = tonumber(Value)
        task.wait(0.1)
    
	 end    
})

Tab:AddButton({
    Name = "Anti Afk",
    Callback = function()
        game:GetService("Players").LocalPlayer.Idled:connect(function()
            vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
            wait(1)
            vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        end)
    end
})
Tab:AddToggle({
	Name = "Auto Open Egg",
	Default = false,
	Callback = function(Value)
        autoEgg = Value 
        if Value then 
            repeat 
                Dispenser.HatchEgg(1)
                task.wait(0.1)
            until not autoEgg 
       end 
	end    
})

local Tab = Window:MakeTab({
    Name = "Player",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
Tab:AddToggle({
    Name = "WalkSpeed",
    Default = false,
    Callback = function(Value)
        walkSpeedEnabled = Value
        if Value then
            LocalPlayer.Character.Humanoid.WalkSpeed = walkSpeed
        else
            LocalPlayer.Character.Humanoid.WalkSpeed = oldSpeed
        end
    end
})
Tab:AddSlider({
    Name = "WalkSpeed",
    Min = 16,
    Max = 100,
    Default = 16,
    Color = Color3.fromRGB(255,255,255),
    Increment = 1,
    ValueName = "IshowSpeed",
    Callback = function(Value)
        walkSpeed = Value
        if walkSpeedEnabled then
            LocalPlayer.Character.Humanoid.WalkSpeed = walkSpeed
        end
    end
})
Tab:AddToggle({
    Name = "JumpPower",
    Default = false,
    Callback = function(Value)
        jumpPowerEnabled = Value
        if Value then
            LocalPlayer.Character.Humanoid.JumpPower = jumpPower
        else
            LocalPlayer.Character.Humanoid.JumpPower = oldJumpPower
        end
    end
})
Tab:AddSlider({
    Name = "JumpPower",
    Min = 50,
    Max = 500,
    Default = 50,
    Color = Color3.fromRGB(255,255,255),
    Increment = 1,
    ValueName = "IshowJump",
    Callback = function(Value)
        jumpPower = Value
        if jumpPowerEnabled then
            LocalPlayer.Character.Humanoid.JumpPower = jumpPower
        end
    end
})
local Tab = Window:MakeTab({
    Name = "Credits",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
Tab:AddParagraph("CynMod#7113","Helping me Get Started")
Tab:AddParagraph("Blake.#1304","Bypasses and Modules")
Tab:AddParagraph("Sigmarillion on v3rm","Anti-afk")

OrionLib:MakeNotification({
	Name = "Annoucement",
	Content = "Must Be in proximity of the egg for the script to work",
	Image = "rbxassetid://4483345998",
	Time = 5
})

